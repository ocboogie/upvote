{
  "name": "upvote",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ext .js,.vue .",
    "format": "npm run lint -- --fix",
    "analyze-client": "vue-cli-service build --report && serve dist/client/report.html",
    "build-client": "vue-cli-service build",
    "build-server": "babel --no-babelrc src/server --presets @babel/env --out-dir dist/server",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "dev-client": "vue-cli-service serve src/client/main.js",
    "dev-server": "nodemon src/server/index.js --watch src/server --exec \"babel-node --no-babelrc --presets @babel/env\"",
    "start": "node dist/server/index.js",
    "heroku-postbuild": "npm run build-server"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "dotenv": "^6.0.0",
    "sequelize": "^5.0.0-beta.12",
    "serve": "^10.0.2",
    "sqlite3": "^4.0.2",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vuex": "^3.0.1",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.2",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "sass-loader": "^7.0.1",
    "vue-cli-plugin-element": "^1.0.0",
    "vue-eslint-parser": "^3.2.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.20.2"
  },
  "engines": {
    "node": "8.12.0"
  }
}
